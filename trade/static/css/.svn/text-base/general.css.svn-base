    float:left;
    padding-left:270px;
}

textarea, select, input {
	float: left;

	margin:6px;
}

textarea {
	border: 1px solid #7f9db9;
}
textarea:focus, input[type="text"]:focus ,input[type="password"]:focus{
	border: 2px solid #7f9db9;
	width:265px;
}
select[type="select-one"] {
	border: 1px solid #7f9db9;
	height:18px;
	background-color: #fff;
	font-family: #3d3b4c Arial, Helvetica, sans-serif;
	font-size: 1.1em;
	width:265px;

}
select[type="select-multiple"] {
	border: 1px solid #7f9db9;
	height:100px;
	background-color: #fff;
	font-family: #3d3b4c Arial, Helvetica, sans-serif;
	font-size: 1.1em;
	width:265px;

}
input[type="text"], input[type="password"] {
	border: 1px solid #7f9db9;
	height:18px;
	background-color: #fff;
	font: 1.1em #3d3b4c Arial, Helvetica, sans-serif;
	width:265px;
}
input[type="submit"] {
	color: #000;
	width:117px;
	height:28px;
	border:0;
}
input[type="checkbox"], input[type="radio"] {
	color: #000;
	background-color:#f0f3f3;
	width:20px;
	float:left;
	line-height: 20px;

}

option {
	background-color:#fff;
	color:#000;
}
optgroup {
	background-color:#ccc;
	color:#000;
}
button {
	background-color: #ccc;
	color: #333;
}

