@charset "utf-8";
/* CSS Document */


/* ----------------- BROWSER RESET ----------------- */

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup,, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, label, legend, {
 margin: 0;
 padding: 0;
 border: 0;
 outline: 0;
 font-size: 100%;
 vertical-align: baseline;
 background: transparent;
}
body {
	line-height: 1;

}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after, q:before, q:after {
	content: '';
	content: none;
}
:focus {
	outline: 0;
}
ins {
	text-decoration: none;
}
del {
	text-decoration: line-through;
}
/* ----------------- GENERALES ----------------- */

body {
	font: 62.5% Arial, Helvetica, sans-serif;
	color: #000;
	background: #FFF;
	margin-left: 100px;
}
a {
	font: 1.2em Arial, Helvetica, sans-serif;
	color:#248FB5;
	text-decoration: none;
}
a:hover {
	text-decoration: none;
	color: #333333;
}
:focus {
	outline: none;
}
ul {
	list-style-type: none;
}
li {
	font-size: 1.1em;
}
input, select, textarea {
	font: 11px Arial, Helvetica, sans-serif;
}

/* ----------------- FOOTER ----------------- */

#footer {
	height:130px;
	padding: 30px 0 0 0;
	display:block;
}
#footer .foot-nav {
	float:left;
	margin-top:60px;
}
#footer .foot-nav ul, #footer .foot-nav p {
	clear:both;
	position:relative;
	display:block;
}
#footer .foot-nav ul {
	margin:10px;
}
#footer .foot-nav ul li {
	float:left;
	height:20px;
 *height:10px;
}
#footer .foot-nav ul li a {
	padding:0 6px;
	font: 1em Arial, Helvetica, sans-serif;
	color: #333333;
	text-decoration:none;
}
#footer .foot-nav ul li a:hover {
	font: 1em Arial, Helvetica, sans-serif;
	color: #248FB5;
	text-decoration:none;
}
#footer .foot-nav p {
	margin:16px;
	height:20px;
	font: 0.9em Arial, Helvetica, sans-serif;
}

